<Window x:Class="abd_2.DataModel.ProstyTextBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:abd_2.DataModel"
        mc:Ignorable="d"
        Title="Wprowadzanie danych" Height="300" Width="300" KeyDown="Window_KeyDown">
    <Grid>
        <TextBox x:Name="WpisanyText" HorizontalAlignment="Left" Height="23" Margin="95,88,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="OKButton" Content="OK" HorizontalAlignment="Left" Margin="95,143,0,0" VerticalAlignment="Top" Width="75" Click="OKButton_Click"/>
        <Label x:Name="NazwaKolumny" Content="Label" HorizontalAlignment="Left" Margin="95,57,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>